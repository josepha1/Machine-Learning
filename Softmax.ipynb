    "import numpy as np\n",
    "\n",
    "def Softmax(L):\n",
    "    '''\n",
    "    INPUTS:\n",
    "    L - List of numbers \n",
    "    \n",
    "    returns: List of numbers given by the softmax function\n",
    "    '''\n",
    "    values = []\n",
    "    for i in L:\n",
    "        values.append((np.exp(i)) / np.sum(np.exp(L)))\n",
    "    return values"
   ]
  }
